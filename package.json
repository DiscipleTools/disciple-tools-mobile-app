{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "./node_modules/.bin/expo start",
    "android": "./node_modules/.bin/expo start --android",
    "ios": "./node_modules/.bin/expo start --ios",
    "lint": "npx eslint --ext .jsx,.js ./ --fix",
    "prettify": "npx pretty-quick --pattern '**/*.*(js|jsx)'",
    "eject": "./node_modules/.bin/expo eject",
    "test": "node ./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation|@react-navigation|expo|@expo|native-base|prop-types)"
    ],
    "automock": false,
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ],
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/netinfo": "~3.2.1",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react-native": "^5.1.9",
    "es6-promise": "^4.2.6",
    "expo": "^35.0.0",
    "expo-asset": "~7.0.0",
    "expo-constants": "~7.0.0",
    "expo-font": "~7.0.0",
    "expo-localization": "~7.0.0",
    "expo-mail-composer": "~7.0.0",
    "expo-web-browser": "~7.0.1",
    "i18n-js": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "native-base": "^2.13.4",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-easy-grid": "^0.2.2",
    "react-native-easy-toast": "^1.2.0",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-material-selectize": "^1.16.0",
    "react-native-modal-filter-picker": "^1.3.4",
    "react-native-progress-bar-animated": "^1.0.6",
    "react-native-reanimated": "~1.2.0",
    "react-native-sticky-keyboard-accessory": "^0.1.3",
    "react-native-tab-view": "^2.10.0",
    "react-native-vector-icons": "^6.4.1",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.1",
    "reactotron-react-native": "^3.6.4",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "redux-saga": "^1.0.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-expo": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.6.0",
    "jest-enzyme": "^7.1.0",
    "jest-expo": "^35.0.0",
    "jest-fetch-mock": "^2.1.2",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "react-dom": "16.8.3",
    "react-test-renderer": "^16.8.4",
    "redux-saga-test-plan": "4.0.0-beta.2",
    "shx": "^0.3.2"
  },
  "private": true
}
